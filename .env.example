HYPERBOLIC_API_KEY=your_hyperbolic_api_key_here
HYPERBOLIC_BASE_URL=https://api.hyperbolic.xyz/v1

# Requested sprint models
HYPERBOLIC_BASE_MODEL=Llama-3.1-405B
HYPERBOLIC_CHAT_MODEL=Llama-3.1-405B-instruct

# Optional fallback chat models for environments where 405B-Instruct is unavailable.
# Comma-separated list.
HYPERBOLIC_CHAT_FALLBACK_MODELS=meta-llama/Meta-Llama-3.1-70B-Instruct,meta-llama/Llama-3.3-70B-Instruct,meta-llama/Meta-Llama-3.1-8B-Instruct

# If false, checker passes when a fallback chat model works.
# If true, checker fails unless exact base/chat models pass.
HYPERBOLIC_REQUIRE_EXACT_MODELS=false
